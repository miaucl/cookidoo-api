{
  "cSpell.words": [
    "aiohttp",
    "aioresponses",
    "asyncio",
    "capsolver",
    "Cookidoo",
    "dotenv",
    "errmsg",
    "gaierror",
    "headful",
    "jlesage",
    "kasmvnc",
    "levelname",
    "miaucl",
    "mypy",
    "networkidle",
    "novnc",
    "recaptcha",
    "swiftshader",
    "venv",
    "Vorwerk"
  ],
  "[python]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit"
    }
  },
  "python.testing.pytestArgs": [
    "-v",
    "--color=yes"
  ],
  "python.analysis.typeCheckingMode": "basic"
}